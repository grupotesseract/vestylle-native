{
  "expo": {
    "name": "Vestylle",
    "slug": "vestylle-native",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.2.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tesseract.vestylle",
      "buildNumber": "9.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QRCodes on promotional tickets."
      }
    },
    "android": {
      "versionCode": 11,
      "package": "com.tesseract.vestylle",
      "googleServicesFile": "./google-services.json"
    },
    "facebookAppId": "654012085033078",
    "notification": {
      "icon": "./assets/icontransparent.png"
    }
  }
}
