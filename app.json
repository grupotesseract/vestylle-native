{
  "expo": {
    "name": "Vestylle",
    "slug": "vestylle-native",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tesseract.vestylle"
    },
    "android": {
      "versionCode": 2,
      "package": "com.tesseract.vestylle",
      "googleServicesFile": "./google-services.json"
    }
  }
}
